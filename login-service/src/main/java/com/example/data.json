create table Phuong_Xa (
Ma_PX varchar(10) not null,
Cap varchar(255) not null,
Ten_PX varchar(255) not null,
Ten_Tieng_Anh varchar(255),
Ma_QH varchar(10) not null,
primary key (Ma_PX)
) engine=InnoDB;

create table Quan_Huyen (
Ma_QH varchar(10) not null,
Ten_QH varchar(255) not null,
Ma_TP varchar(10) not null,
primary key (Ma_QH)
) engine=InnoDB;

create table Tinh_Thanh_Pho (
Ma_TP varchar(10) not null,
Ten_TP varchar(255) not null,
primary key (Ma_TP)
) engine=InnoDB;

